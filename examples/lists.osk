list = [1,2,3]
print(list)

-- Change the value of an element
list[0] = 10
print(list)

-- Add an element to the end of the list
list[3] = 10
print(list)

deep = [1,2,3,[4,5,6]]
print(deep)

-- Change the value of a nested list
deep[3][0] = 10
print(deep)

-- Add another nested list
deep[3][0] = [10,11,12]
print(deep)

-- Reference to the nested list mutates the original list
ref = deep[3]
ref[0][0] = 13
print(deep)